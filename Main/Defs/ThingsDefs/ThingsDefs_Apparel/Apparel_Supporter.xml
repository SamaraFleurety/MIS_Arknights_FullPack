	<ThingDef ParentName="AK_ArmorBase_Supporter">
		<defName>AK_Apparel_Raidian</defName>
		<label>电弧服装</label>
		<thingClass>AKE_OperatorExtension.Apparel_Breakdown</thingClass>
		<useHitPoints>true</useHitPoints>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
		</statBases>
		<modExtensions>
			<li Class="AKE_OperatorExtension.Ext_ApparelBreakGraphic">
				<paths>
					<li>
						<path>Things/AK_Agents/Supporter/Raidian/Raidian_Naked</path>
						<hpRatioLowerThan>0.2</hpRatioLowerThan>
					</li>
					<li>
						<path>Things/AK_Agents/Supporter/Raidian/Raidian_Break</path>
						<hpRatioLowerThan>0.8</hpRatioLowerThan> 
					</li>
					<li>
						<path>Things/AK_Agents/Supporter/Raidian/Raidian</path>
						<hpRatioLowerThan>1</hpRatioLowerThan> 
					</li>
				</paths>
			</li>
		</modExtensions>
		<graphicData>
			<texPath>Things/AK_Agents/Supporter/Raidian/Raidian</texPath>
